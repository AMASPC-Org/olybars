{
  "name": "olybars-production",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node --import tsx server/src/index.ts",
    "server": "nodemon --exec tsx server/src/index.ts",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "smoke:dev": "curl.exe -fI https://olybars-dev.web.app/ && curl.exe -f https://olybars-backend-juthzlaerq-uw.a.run.app/health && curl.exe -f https://olybars-backend-juthzlaerq-uw.a.run.app/api/venues",
    "smoke:prod": "curl.exe -fI https://olybars.com/ && curl.exe -f https://olybars-backend-26629455103.us-west1.run.app/health && curl.exe -f https://olybars-backend-26629455103.us-west1.run.app/api/venues",
    "deploy:dev": "npm run build:dev && node scripts/check-env.js development && firebase deploy --only hosting:dev",
    "deploy:prod": "npm run build:prod && node scripts/check-env.js production && firebase deploy --only hosting:prod",
    "deploy:backend": "gcloud builds submit --tag us-west1-docker.pkg.dev/ama-ecosystem-prod/olybars/backend:latest . && gcloud run deploy olybars-backend --image us-west1-docker.pkg.dev/ama-ecosystem-prod/olybars/backend:latest --region us-west1 --allow-unauthenticated",
    "generate-sitemap": "tsx scripts/generate-sitemap.ts"
  },
  "dependencies": {
    "@genkit-ai/ai": "^1.27.0",
    "@genkit-ai/core": "^1.27.0",
    "@genkit-ai/dotprompt": "^0.9.12",
    "@genkit-ai/firebase": "^1.27.0",
    "@genkit-ai/flow": "^0.5.17",
    "@genkit-ai/googleai": "^1.27.0",
    "@google/genai": "^1.34.0",
    "@tanstack/react-query": "^5.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "firebase": "^12.7.0",
    "firebase-admin": "^13.6.0",
    "lucide-react": "^0.454.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "genkit-cli": "^1.27.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "vite": "^5.2.10"
  }
}